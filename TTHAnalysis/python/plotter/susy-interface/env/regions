BEGIN::2losEwkLow
  mccs        +:= susy-sos-v1/mcc_triggerdefs.txt ; susy-sos-v1/2l/mcc_2l.txt ; susy-sos-v1/mcc_sos.txt ; susy-sos-v1/2l/mcc_sf_met125.txt ; susy-sos-v1/2l/mcc_sf_fakes_2losEwkLow.txt
  expr         := m2lSel
  bins         := [4,10,20,30,50]
  bkgs        +:= prompt_.*;fakes_appldata;rares
  sigs        +:= 
  flags       +:= -E SF -E mT -E pt5sublep -E met125_met -E met125_mumu -E met125_trig -E met125_run 
  flagsPlots  +:= --legendHeader 'SR 2lOS: 125 #leq #it{p}_{T}^{miss} < 200 GeV'
  plots       #:= {perCateg : SR_2l_ewk                                       }
  plots       #:= {evt      : mt1; mt2                                        }
  fmt          := {LepSel1_pdgId:+2d} {LepSel1_pt:5.1f} {LepSel1_mcUCSX:2d}\\t{LepSel2_pdgId:+2d} {LepSel2_pt:5.1f} {LepSel2_dz:1.5f} {LepSel2_mcUCSX:2d}\\t{LepSel3_pdgId:+2d} {LepSel3_pt:5.1f} {LepSel3_mcUCSX:2d}\\t{nJet30:d}\\t{nBJetMedium25:2d}\\t{met_pt:5.1f}\\t{htJet30j:6.1f}\\t{mll_3l:6.1f}\\t{mT_3l:6.1f}\\t{mT2L_3l:6.1f}\\t{mT2T_3l:6.1f}
ENDDEF

